@{
    Bundles.Reference("Extends/CommonJS", "global");
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="renderer" content="webkit" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="keywords" content="@ViewBag.Keywords" />
    <meta name="description" content="@ViewBag.Description" />
    <title>@ViewBag.Title</title>
    <link rel="shortcut icon" href="@ViewBag.Favicon" />

    <link href="@ViewBag.BaseUrl/Reference/antd/antd.min.css" rel="stylesheet" />
    <link href="@ViewBag.BaseUrl/Reference/leaflet/leaflet.css" rel="stylesheet" />
    <link href="@ViewBag.BaseUrl/Reference/ctfont/iconfont.css" rel="stylesheet" />
    <link href="@ViewBag.BaseUrl/Reference/jquery-easyui/themes/metro/easyui.css" rel="stylesheet" />
    <link href="@ViewBag.BaseUrl/Extends/globalStyle.css" rel="stylesheet" />

    <script src="@ViewBag.BaseUrl/Extends/CommonJS/jsExtends.js"></script>
    <script src="@ViewBag.BaseUrl/Reference/jquery-easyui/jquery.min.js"></script>
    <script src="@ViewBag.BaseUrl/Reference/react/react.min.js"></script>
    <script src="@ViewBag.BaseUrl/Reference/react/react-dom.min.js"></script>
    <script src="@ViewBag.BaseUrl/Reference/antd/antd.min.js"></script>
    <script src="@ViewBag.BaseUrl/Reference/is.min.js"></script>
    <script src="@ViewBag.BaseUrl/Reference/jquery-easyui/jquery.easyui.min.js"></script>
    <script src="@ViewBag.BaseUrl/Reference/jquery-easyui/locale/easyui-lang-zh_CN.js"></script>

    <script>
        var _bl_="@ViewBag.BaseUrl";
        var mapConfig=@Html.Raw(@ViewBag.Map);
        var djConfig = {
            serverIP:"@ViewBag.ServerIP"
        };
    </script>
    <link href="@ViewBag.ArcGISApiStyle" rel="stylesheet" />
    <script src="@ViewBag.ArcGISApiScript"></script>
    
    @Bundles.RenderStylesheets("global")
    @Bundles.RenderScripts("global")
</head>
<body>
    @RenderBody()
</body>
</html>
